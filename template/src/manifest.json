{
  "manifest_version": 2,
  "name": "{{name}}",
  "description": "{{description}}",
  "version": "{{version}}",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "{{displayName}}",
    "default_popup": "popup.html",
    "default_icon": "icon-256.png"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  }
}
